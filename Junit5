import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;
import static org.junit.jupiter.api.Assertions.assertNotEquals;


public class SpaceCollectorTest {

    @Test
    public void testEquals_sameObject() {
        SpaceCollector spaceCollector = new SpaceCollector("inputFile.txt", 1, "http://example.com", 8080, "outputFile.txt", ",", "txt");
        assertEquals(spaceCollector, spaceCollector);
    }

    @Test
    public void testEquals_equalObjects() {
        SpaceCollector spaceCollector1 = new SpaceCollector("inputFile.txt", 1, "http://example.com", 8080, "outputFile.txt", ",", "txt");
        SpaceCollector spaceCollector2 = new SpaceCollector("inputFile.txt", 1, "http://example.com", 8080, "outputFile.txt", ",", "txt");
        assertEquals(spaceCollector1, spaceCollector2);
    }

    @Test
    public void testEquals_notEqualObjects() {
        SpaceCollector spaceCollector1 = new SpaceCollector("inputFile.txt", 1, "http://example.com", 8080, "outputFile.txt", ",", "txt");
        SpaceCollector spaceCollector2 = new SpaceCollector("inputFile2.txt", 2, "http://example2.com", 8181, "outputFile2.txt", " ", "csv");
        assertNotEquals(spaceCollector1, spaceCollector2);
    }

    @Test
    public void testEquals_nullObject() {
        SpaceCollector spaceCollector = new SpaceCollector("inputFile.txt", 1, "http://example.com", 8080, "outputFile.txt", ",", "txt");
        assertNotEquals(spaceCollector, null);
    }

    @Test
    public void testEquals_differentClassObject() {
        SpaceCollector spaceCollector = new SpaceCollector("inputFile.txt", 1, "http://example.com", 8080, "outputFile.txt", ",", "txt");
        assertNotEquals(spaceCollector, new Object());
    }

    @Test
    public void testEquals_equalObjectsHashCode() {
        SpaceCollector spaceCollector1 = new SpaceCollector("inputFile.txt", 1, "http://example.com", 8080, "outputFile.txt", ",", "txt");
        SpaceCollector spaceCollector2 = new SpaceCollector("inputFile.txt", 1, "http://example.com", 8080, "outputFile.txt", ",", "txt");
        assertEquals(spaceCollector1.hashCode(), spaceCollector2.hashCode());
    }

}