Here is the Cucumber glue code steps for the given MRFC CSV file records:


@Given(/^a csv file with MRFC data$/)
public void a_csv_file_with_MRFC_data() {
  // Given a csv file with MRFC data
  csvFile = new CsvFile("mrfc.csv");
}

@When(/^CsvtoJsonMRFCnTransformer is invoked on the csv file$/)
public void csvtojsonmrfcntransformer_is_invoked_on_the_csv_file() {
  // When CsvtoJsonMRFCnTransformer is invoked on the csv file
  transformer = new CsvtoJsonMRFCnTransformer();
  transformer.transform(csvFile);
}

@And(/^the MRFC csv file content is converted to json$/)
public void the_MRFC_csv_file_content_is_converted_to_json() {
  // And the MRFC csv file content is converted to json
  json = transformer.getJson();
}

@Then(/^the converted MRFC json content should be valid$/)
public void the_converted_MRFC_json_content_should_be_valid() {
  // Then the converted MRFC json content should be valid
  assertThat(json).isNotNull();
  assertThat(json).hasSize(10);
  assertThat(json.get(0).get("SystemId")).isEqualTo("SOLKTXE1100.101");
  assertThat(json.get(0).get("NodeIP")).isEqualTo("2607:f160:8a03:a03a:c0:10f::");
  assertThat(json.get(0).get("STARTTIMESTAMP")).isEqualTo("2024-01-10T17:05:00");
  assertThat(json.get(0).get("STOPTIMESTAMP")).isEqualTo("2024-01-10T17:10:00");
  assertThat(json.get(0).get("GATEWAYNUM")).isEqualTo(7);
  assertThat(json.get(0).get("SERVERID")).isEqualTo(0);
  assertThat(json.get(0).get("RGN")).isEqualTo(7);
  assertThat(json.get(0).get("NBROFSVRAUDIT")).isEqualTo(10);
  assertThat(json.get(0).get("NBROFSVRAUDITFAIL")).isEqualTo(0);
  assertThat(json.get(0).get("NBROFDNSQUERY")).isEqualTo(10);
  assertThat(json.get(0).get("NBROFONSQUERYFAIL")).isEqualTo(0);
  assertThat(json.get(0).get("NBROFENUMQUERY")).isEqualTo(10);
  assertThat(json.get(0).get("NBROFENUMQUERYFAIL")).isEqualTo(0);
  assertThat(json.get(0).get("NBROFDNSARECORDQUERY")).isEqualTo(10);
  assertThat(json.get(0).get("NBROFDNSARECORDQUERYFAIL")).isEqualTo(0);
  assertThat(json.get(0).get("NBROFDNSAAAARECORDQUERY")).isEqualTo(10);
  assertThat(json.get(0).get("NBROFDNSAAAAARECORDQUERYFAIL")).isEqualTo(0);
  assertThat(json.get(0).get("NBROFDNSSRVRECORDQUERY")).isEqualTo(10);
  assertThat(json.get(0).get("NBROFDNSSRVRECORDQUERYFAIL")).isEqualTo(0);
  assertThat(json.get(0).get("NBROFDNSSOARECORDQUERY")).isEqualTo(10);
  assertThat(json.get(0).get("NBROFDNSSOARECORDQUERYFAIL")).isEqualTo(0);
}