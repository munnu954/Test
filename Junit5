@Test
public void testGetClrGraphics_CatchBlock() {
    // Setup
    CommonWrapperController commonWrapperController = new CommonWrapperController();
    ClrGraphicsService mockClrGraphicsService = mock(ClrGraphicsService.class);
    commonWrapperController.clrGraphicsService = mockClrGraphicsService;
    
    String circuitId = "testCircuitId";
    String sourceSys = "testSourceSys";
    String cnd = "testCnd";
    String phyModel = "testPhyModel";
    
    // Mock behavior
    when(mockClrGraphicsService.getClrGraphics(anyString(), anyString(), anyString(), anyString())).thenThrow(new UnsupportedEncodingException());
    
    // Execution
    String response = commonWrapperController.getClrGraphics(circuitId, sourceSys, cnd, phyModel);
    
    // Verification
    verify(mockClrGraphicsService, times(1)).getClrGraphics(circuitId, sourceSys, "N", phyModel);
    assertThat(response).isNull();
}
