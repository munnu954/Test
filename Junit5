import com.fasterxml.jackson.databind.ObjectMapper;

ObjectMapper objectMapper = new ObjectMapper();
for (Map.Entry<String, Object> entry : hashMap.entrySet()) {
    String key = entry.getKey();
    String jsonString = entry.getValue().toString(); // Convert Object to String

    try {
        // Parse JSON string into a Map or JsonNode
        Map<String, Object> jsonMap = objectMapper.readValue(jsonString, Map.class);
        System.out.println("Key: " + key + ", Parsed JSON: " + jsonMap);
    } catch (Exception e) {
        System.err.println("Error parsing JSON for key " + key + ": " + e.getMessage());
    }
}