    @BeforeEach
    void setup() {
        MockitoAnnotations.initMocks(this);
    }

    @Test
    void testPostAlarmForEachIteration() {
        Alarm alarm = new Alarm();
        alarm.setAlarmIdentity("testAlarm");
        alarm.setAlarmUpdatedTime("2022-01-01T12:00:00Z");
        String compId = "testCompId";

        when(jdbcTemplate.queryForRowSet(anyString(), any(Object[].class))).thenReturn(null);

        try {
            int result = alarmRepositoryImpl.postAlarmForEachIteration(alarm, compId);
            assertEquals(-1, result);
        } catch (Exception e) {
            // Handle exception
        }
    }
}
