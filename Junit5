@Test
void testGetClrGraphicsException() throws Exception {
    // Arrange
    String circuitId = "testCircuit";
    String sourceSys = "testSourceSys";
    String cnd = "Y";

    when(URLDecoder.decode(anyString(), eq(StandardCharsets.UTF_8.toString())))
            .thenThrow(new UnsupportedEncodingException("UnsupportedEncodingException"));

    // Act
    String response = commonWrapperController.getClrGraphics(circuitId, sourceSys, cnd);

    // Assert
    assertNotNull(response);
    assertEquals("", response);
}
