        String json = "{ \"externalAlarmId\":\"pqwer\", \"State\":\"cleared\", \"sequenceId\":\"23423\", \"reportingObject\":null, \"alarmedObject\":\"someAlarmedObject\", \"tid\":\"someTid\"}";

