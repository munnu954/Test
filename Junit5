import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class ConstantsTest {

    @Test
    public void testRetryCountConstant() {
        assertEquals(5, Constants.RETRY_COUNT, "Retry count constant should be 5");
    }

    @Test
    public void testProtocolConfigConstant() {
        assertEquals("SASL_SSL", Constants.PROTOCOL_CONFIG, "Protocol config constant should be SASL_SSL");
    }
}