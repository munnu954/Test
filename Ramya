public class UnifiedAuditMessage {
    private ProcessType processType;
    private String inputFilePath;
    private String ur;
    private Integer port;
    private String outputFilePath;
    private String delimiters;
    private String fileType;
    private UUID collectorId;
    private JobStatus jobStatus;
    private List<String> exceptions;
    private LocalDateTime dateReceived;
    private LocalDateTime dateProcessed;
    private UUID jobId;
    private UUID jarId;
    private Integer numOfRecords;
    private Integer numOfRecordsProcessed;
    private Integer numOfRecordsFailed;

    // Setters
    public void setProcessType(ProcessType processType) {
        this.processType = processType;
    }

    public void setInputFilePath(String inputFilePath) {
        this.inputFilePath = inputFilePath;
    }

    public void setUr(String ur) {
        this.ur = ur;
    }

    public void setPort(Integer port) {
        this.port = port;
    }

    public void setOutputFilePath(String outputFilePath) {
        this.outputFilePath = outputFilePath;
    }

    public void setDelimiters(String delimiters) {
        this.delimiters = delimiters;
    }

    public void setFileType(String fileType) {
        this.fileType = fileType;
    }

    public void setCollectorId(UUID collectorId) {
        this.collectorId = collectorId;
    }

    public void setJobStatus(JobStatus jobStatus) {
        this.jobStatus = jobStatus;
    }

    public void setExceptions(List<String> exceptions) {
        this.exceptions = exceptions;
    }

    public void setDateReceived(LocalDateTime dateReceived) {
        this.dateReceived = dateReceived;
    }

    public void setDateProcessed(LocalDateTime dateProcessed) {
        this.dateProcessed = dateProcessed;
    }

    public void setJobId(UUID jobId) {
        this.jobId = jobId;
    }

    public void setJarId(UUID jarId) {
        this.jarId = jarId;
    }

    public void setNumOfRecords(Integer numOfRecords) {
        this.numOfRecords = numOfRecords;
    }

    public void setNumOfRecordsProcessed(Integer numOfRecordsProcessed) {
        this.numOfRecordsProcessed = numOfRecordsProcessed;
    }

    public void setNumOfRecordsFailed(Integer numOfRecordsFailed) {
        this.numOfRecordsFailed = numOfRecordsFailed;
    }

    // Getters
    public ProcessType getProcessType() {
        return processType;
    }

    public String getInputFilePath() {
        return inputFilePath;
    }

    public String getUr() {
        return ur;
    }

    public Integer getPort() {
        return port;
    }

    public String getOutputFilePath() {
        return outputFilePath;
    }

    public String getDelimiters() {
        return delimiters;
    }

    public String getFileType() {
        return fileType;
    }

    public UUID getCollectorId() {
        return collectorId;
    }

    public JobStatus getJobStatus() {
        return jobStatus;
    }

    public List<String> getExceptions() {
        return exceptions;
    }

    public LocalDateTime getDateReceived() {
        return dateReceived;
    }

    public LocalDateTime getDateProcessed() {
        return dateProcessed;
    }

    public UUID getJobId() {
        return jobId;
    }

    public UUID getJarId() {
        return jarId;
    }

    public Integer getNumOfRecords() {
        return numOfRecords;
    }

    public Integer getNumOfRecordsProcessed() {
        return numOfRecordsProcessed;
    }

    public Integer getNumOfRecordsFailed() {
        return numOfRecordsFailed;
    }
}
