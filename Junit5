        cable.setType("Type");
        cable.setStatus("Active");
        cableComponent.getCableComp().add(cable);
        clrComponentList.add(cableComponent);
        
        cableProcessor.process(nodes, links, clrComponent, clrComponentList, clliNodeMap, 0, ukCount, sourceSys);
        
