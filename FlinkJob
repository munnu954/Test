@Override
public void flatMap(String s, Collector<String> collector) throws Exception {
    String[] lines = s.split(System.lineSeparator());
    for (String str : lines) {
        String[] arr = str.split(",");
        if (headers == null) {
            headers = new String[arr.length]; // Initialize headers array
            for (int i = 0; i < arr.length; i++) {
                headers[i] = String.valueOf(i); // Use index as header key
            }
        } else {
            if (headers.length == arr.length) {
                JSONObject obj = new JSONObject();
                for (int i = 0; i < arr.length; i++) {
                    String jsonkey = headers[i]; // Get header key from array
                    String jsonvalue = arr[i].trim().replace("\"", "");
                    obj.put(jsonkey, jsonvalue);
                }
                this.successIntCounter.add(1);
                collector.collect(obj.toString());
            } else {
                this.failureIntCounter.add(1);
            }
        }
    }
}