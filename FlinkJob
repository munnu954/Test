@Override
public void flatMap(String s, Collector<String> collector) throws Exception {
    String[] lines = s.split(System.lineSeparator());
    for (String str : lines) {
        String[] arr = str.split(",");
        JSONObject obj = new JSONObject();
        boolean validLine = true;
        for (int i = 0; i < arr.length; i++) {
            String jsonkey = String.valueOf(i);
            String jsonvalue = arr[i].trim().replace("\"", "");
            obj.put(jsonkey, jsonvalue);
        }
        if (validLine) {
            // Sort the keys in ascending order
            List<String> sortedKeys = new ArrayList<>();
            Iterator<String> keys = obj.keys();
            while (keys.hasNext()) {
                sortedKeys.add(keys.next());
            }
            Collections.sort(sortedKeys);
            
            // Create a new JSONObject with sorted keys
            JSONObject sortedObj = new JSONObject();
            for (String key : sortedKeys) {
                sortedObj.put(key, obj.get(key));
            }
            
            this.successIntCounter.add(1);
            collector.collect(sortedObj.toString());
        } else {
            this.failureIntCounter.add(1);
        }
    }
}