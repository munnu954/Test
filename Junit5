        String cnd = "Y";
        String phyModel = "model";

        // Act
        String response = commonWrapperController.getClrGraphics(circuitId, sourceSys, cnd, phyModel);

        // Assert
        verify(clrGraphicsService, times(1)).getClrGraphics(circuitId, sourceSys, cnd, phyModel);
    }

    @Test
    void testGetClrGraphics_PhyModelNull() {
        // Arrange
        String circuitId = "123";
        String sourceSys = "source";
        String cnd = "Y";
        String phyModel = null;

        // Act
