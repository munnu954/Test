import org.junit.jupiter.api.Test;
import org.mockito.Mock;
import org.springframework.boot.SpringApplication;

import static org.mockito.Mockito.*;

public class SshServerApplicationTest {

    @Test
    public void testMain() {
        // Mock SpringApplication class
        SpringApplication springApplication = mock(SpringApplication.class);

        // Call the main method of SshServerApplication
        SshServerApplication.main(new String[]{});

        // Verify that SpringApplication.run method is called with correct arguments
        verify(springApplication, times(1)).run(eq(SshServerApplication.class), any(String[].class));
    }
}