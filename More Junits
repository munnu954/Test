Here is a JUnit test case to ReadCSVFile (CSV file with some 10000 entries) and print the csv file data:


import org.junit.Test;

import java.io.File;
import java.io.IOException;
import java.io.InputStream;
import java.io.PrintWriter;
import java.util.ArrayList;
import java.util.List;

import static org.junit.Assert.assertEquals;

public class ReadCSVFileTest {

    @Test
    public void testReadCSVFile() throws IOException {
        // Arrange
        String csvFilePath = "src/test/resources/test.csv";
        List expectedLines = new ArrayList<>();
        expectedLines.add("1,2,3");
        expectedLines.add("4,5,6");
        expectedLines.add("7,8,9");

        // Act
        List actualLines = ReadCSVFile(csvFilePath);

        // Assert
        assertEquals(expectedLines, actualLines);
    }

    public static List ReadCSVFile(String csvFilePath) throws IOException {
        // Open the CSV file
        InputStream inputStream = new FileInputStream(csvFilePath);

        // Create a reader for the CSV file
        BufferedReader reader = new BufferedReader(new InputStreamReader(inputStream));

        // Create a list to store the lines of the CSV file
        List lines = new ArrayList<>();

        // Read each line of the CSV file
        String line;
        while ((line = reader.readLine()) != null) {
            lines.add(line);
        }

        // Close the reader
        reader.close();

        // Return the list of lines
        return lines;
    }
}