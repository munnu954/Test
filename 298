import org.apache.flink.client.JobClient;


@Test
public void testUploadTextToJsonJob() throws Exception {

    // Given
    String jobName = "TextToJsonJob";
    String jobPath = "/path/to/job/jar";

    // When
    FlinkJobClient jobClient = new FlinkJobClient();
    jobClient.uploadJob(jobName, jobPath);

    // Then
    assertTrue(jobClient.isJobAvailable(jobName));
}