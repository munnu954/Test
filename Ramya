// Process lines and generate JSON
for (int i = 1; i < lines.length; i++) {
    String[] arr = lines[i].split(",");

    if (arr.length > 2 && !arr[2].isEmpty()) {
        // This is the line containing ObjectName
        objectName = arr[2].trim();
        JsonObject objectDetails = new JsonObject();

        // Extract details from the lines below ObjectName
        if (i + 3 < lines.length) {
            objectDetails.addProperty("Begin Date", lines[i + 1].split(",")[1].trim());
            objectDetails.addProperty("End Date", lines[i + 2].split(",")[1].trim());
            objectDetails.addProperty("Ticket Number", lines[i + 3].split(",")[1].trim());
        }

        // Process sections and add to objectDetails
        while (i + 1 < lines.length && !lines[i + 1].startsWith(",")) {
            i++;

            // Check if there are enough elements in the array
            if (i + 1 < lines.length) {
                String sectionName = lines[i].trim();
                String[] sectionArr = lines[i + 1].split(",");
                JsonObject sectionObject = new JsonObject();

                // Check if there are enough elements in the sectionArr
                if (sectionArr.length > 3) {
                    sectionObject.addProperty("Min", sectionArr[3].trim());
                    sectionObject.addProperty("Average", sectionArr[4].trim());
                    sectionObject.addProperty("Max", sectionArr[5].trim());
                    sectionObject.addProperty("Cumulative Counter Value", sectionArr[6].trim());
                }

                objectDetails.add(sectionName, sectionObject);
            }
        }

        // Add the final JSON structure to result
        finalResult.add(objectName, objectDetails);
    }
}

