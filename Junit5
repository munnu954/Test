    String circuitId = "testCircuitId";
    String sourceSys = "testSourceSys";
    String phyModel = "testPhyModel";
    String cnd = "N"; // cnd is not blank but not equal to "Y"
    when(service.getClrGraphics(anyString(), anyString(), anyString(), anyString())).thenReturn("response");
    
    String response = controller.getClrGraphics(circuitId, sourceSys, cnd, phyModel);
    
    Assertions.assertNotNull(response);
    verify(service, times(1)).getClrGraphics(circuitId, sourceSys, "N", phyModel);
}
