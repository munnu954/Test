import java.util.*;

public class Main {

    public static String StringChallenge(String str) {
        Stack<String> stack = new Stack<>();
        HashMap<String, String> matchingTags = new HashMap<>();
        matchingTags.put("b", "b");
        matchingTags.put("i", "i");
        matchingTags.put("em", "em");
        matchingTags.put("div", "div");
        matchingTags.put("p", "p");
        StringBuilder output = new StringBuilder();
        
        for (int i = 0; i < str.length(); i++) {
            char ch = str.charAt(i);
            
            if (ch == '<') {
                int endIndex = str.indexOf('>', i);
                if (endIndex == -1) {
                    return "Invalid input";
                }
                String tag = str.substring(i + 1, endIndex);
                if (tag.charAt(0) == '/') {
                    String openingTag = stack.pop();
                    if (!matchingTags.get(openingTag).equals(tag.substring(1))) {
                        output.append(openingTag);
                        break;
                    }
                } else {
                    stack.push(tag);
                }
                i = endIndex; // Move i to end of the tag
            }
        }
        
        if (!stack.isEmpty()) {
            output.append(stack.pop());
        }
        
        String reversedToken = new StringBuilder("fd5y9lo3txu").reverse().toString();
        return output.toString() + ":" + reversedToken;
    }

    public static void main(String[] args) {
        // keep this function call here
        Scanner s = new Scanner(System.in);
        System.out.print(StringChallenge(s.nextLine()));
    }
}