@Test
void testParseWithNoAttributeMatch() throws Exception {
// Arrange
String expression = "%nonExistent:attribute%";
// Act
String result = alarmPostingUtils.parse(expression, srcMap, "");
// Assert
assertEquals("", result);
}

@Test
void testParseWithNoJsonFieldMatch() throws Exception {
// Arrange
String expression = "%attribute1:details.unknown%";
srcMap.put("details", Map.of("name", "John"));
// Act
String result = alarmPostingUtils.parse(expression, srcMap, "");
// Assert
assertEquals("", result);
}

@Test
void testParseWithNoAttributeMapMatch() throws Exception {
// Arrange
String expression = "%attribute1:details.name%";
// Act
String result = alarmPostingUtils.parse(expression, srcMap, "");
// Assert
assertEquals("", result);
}

@Test
void testParseWithInvalidArrayIndex() throws Exception {
// Arrange
String expression = "%attribute1:names [5]%";
srcMap.put("names", List.of("John", "Jane"));
// Act
String result = alarmPostingUtils.parse(expression, srcMap, "");
// Assert
assertEquals("", result);
}