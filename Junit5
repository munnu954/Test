
@Test
public void testGetFlinkJobDetails_withValidFileType_andEnabled_andSortField_andSortOrder() {
  // Arrange
  String fileType = "text";
  boolean enabled = true;
  String sortField = "id";
  String sortOrder = "asc";

  // Act
  FlinkJob flinkJob = getFlinkJobDetails(fileType, enabled, sortField, sortOrder);

  // Assert
  assertNotNull(flinkJob);
  assertEquals(fileType, flinkJob.getType());
  assertTrue(flinkJob.isEnabled());
  assertEquals(sortField, flinkJob.getSortField());
  assertEquals(sortOrder, flinkJob.getSortOrder());
}

@Test
public void testGetFlinkJobDetails_withValidFileType_andEnabled_andSortField_andSortOrder_andNoResults() {
  // Arrange
  String fileType = "text";
  boolean enabled = true;
  String sortField = "id";
  String sortOrder = "asc";

  // Act
  List flinkJobList = getFlinkJobDetails(fileType, enabled, sortField, sortOrder);

  // Assert
  assertTrue(flinkJobList.isEmpty());
}

@Test
public void testGetFlinkJobDetails_withInvalidFileType() {
  // Arrange
  String fileType = "invalid";
  boolean enabled = true;
  String sortField = "id";
  String sortOrder = "asc";

  // Act
  List flinkJobList = getFlinkJobDetails(fileType, enabled, sortField, sortOrder);

  // Assert
  assertTrue(flinkJobList.isEmpty());
}

@Test
public void testGetFlinkJobDetails_withInvalidEnabled() {
  // Arrange
  String fileType = "text";
  boolean enabled = false;
  String sortField = "id";
  String sortOrder = "asc";

  // Act
  List flinkJobList = getFlinkJobDetails(fileType, enabled, sortField, sortOrder);

  // Assert
  assertTrue(flinkJobList.isEmpty());
}

