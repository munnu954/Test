    @Test
    void testParseWithEmptyString() throws Exception {
        // Given
        String value = "%some_key%";
        Map<String, Object> srcMap = new HashMap<>();
        String ruleName = "testRule";
        
        // Creating a mock object for ObjectMapper
        ObjectMapper objectMapperMock = mock(ObjectMapper.class);
        when(RulesConstants.getObjectMapper()).thenReturn(objectMapperMock);

        // When
        String result = AlarmPostingUtils.parse(value, srcMap, ruleName);

        // Then
        assertEquals("", result);
    }
}
