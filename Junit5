@Test
    public void testGetClrGraphics_UnsupportedEncodingException() throws Exception {
        String circuitId = "invalidCircuitId%";
        String sourceSys = "testSourceSys";
        String cnd = "Y";
        String phyModel = "testPhyModel";

        // Mock URLDecoder.decode to throw UnsupportedEncodingException
        try (MockedStatic<URLDecoder> urlDecoderMock = mockStatic(URLDecoder.class)) {
            urlDecoderMock.when(() -> URLDecoder.decode(anyString(), anyString()))
                          .thenThrow(new UnsupportedEncodingException("Error while parsing circuitId"));

            String result = commonWrapperController.getClrGraphics(circuitId, sourceSys, cnd, phyModel);

            assertNull(result);
        }