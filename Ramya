public class CustomPojo {
    private String beginDate;
    private String endDate;
    private int ticketNumber;
    // other fields

    // getters and setters
}

public class CsvLineToPojoMapper implements MapFunction<String, CustomPojo> {
    @Override
    public CustomPojo map(String value) throws Exception {
        String[] fields = value.split(",");
        CustomPojo pojo = new CustomPojo();
        pojo.setBeginDate(fields[0]);
        pojo.setEndDate(fields[1]);
        pojo.setTicketNumber(Integer.parseInt(fields[2]));
        // other fields
        return pojo;
    }
}