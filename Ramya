// Check if the extracted details are valid
boolean isInvalidDetails = objectDetails.containsKey("Begin Date")
        && objectDetails.containsKey("End Date")
        && objectDetails.containsKey("Ticket Number");

// Your condition to emit to side output
if (!isInvalidDetails) {
    // Emit to side output
    getRuntimeContext().getSideOutput(sideOutputTag).collect(result.toString());
} else {
    // Continue processing and collect to the main output
    result.put("ObjectName", objectName);
    result.put("Begin Date", objectDetails.getString("Begin Date"));
    result.put("End Date", objectDetails.getString("End Date"));
    result.put("Ticket Number", objectDetails.getString("Ticket Number"));
    objectDetails.remove("Begin Date");
    objectDetails.remove("End Date");
    objectDetails.remove("Ticket Number");
    result.put(objectName, objectDetails);

    collector.collect(result.toString());
}
