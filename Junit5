Sure, here are the Junit5 test cases covering all the if statements of the convertToMtnInsights(List<MtnInsightsEntity> entities, boolean flag) method of the MtnInsightsManager class for 100% code coverage, including try and catch blocks:
import static org.junit.jupiter.api.Assertions.*;
import static org.mockito.Mockito.*;

import java.util.ArrayList;
import java.util.List;

import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;
import org.mockito.Mock;
import org.mockito.MockitoAnnotations;

public class MtnInsightsManagerTest {

    @Mock
    private MtnInsightsRepository insightsRepository;
    @Mock
    private AggregateInsightsRepository aggrInsightRepository;
    @Mock
    private EcpdByKeyRepository ecpdByKeyRepository;
    @Mock
    private MtnInsightsValidator validator;

    private MtnInsightsManager manager;

    @BeforeEach
    public void setUp() {
        MockitoAnnotations.openMocks(this);
        manager = new MtnInsightsManager();
        manager.insightsRepository = insightsRepository;
        manager.aggrInsightRepository = aggrInsightRepository;
        manager.ecpdByKeyRepository = ecpdByKeyRepository;
        manager.validator = validator;
    }

    // Test case for the first if condition: set.getKey().equals(INSIGHT_CATEGORY_NETWORK_OUTAGE)
    @Test
    public void testConvertToMtnInsights_NetworkOutage() {
        List<MtnInsightsEntity> entities = createNetworkOutageEntities();
        boolean flag = true;

        InsightsMtn insights = manager.convertToMtnInsights(entities, flag);

        // Verify interactions with mock repositories
        verify(insightsRepository, times(1)).findByPkEcpdIdAndPkMtnAndPkInsightCategoryInAndPkInsightNameIn(anyString(), anyString(), anyString(), anyString());
        verify(aggrInsightRepository, times(1)).findByPkAggrCategoryAndPkAggrProdId(anyString(), anyString());

        // Assert that insights are not null
        assertNotNull(insights);
    }

    // Test case for the second if condition: !StringUtils.equals(pk.getInsightName(), MTNOUTAGECASE) && !StringUtils.equals(pk.getInsightName(), MTNOUTAGE)
    @Test
    public void testConvertToMtnInsights_OtherInsights() {
        List<MtnInsightsEntity> entities = createOtherInsightsEntities();
        boolean flag = true;

        InsightsMtn insights = manager.convertToMtnInsights(entities, flag);

        // Verify interactions with mock repositories
        verify(insightsRepository, never()).findByPkEcpdIdAndPkMtnAndPkInsightCategoryInAndPkInsightNameIn(anyString(), anyString(), anyString(), anyString());
        verify(aggrInsightRepository, never()).findByPkAggrCategoryAndPkAggrProdId(anyString(), anyString());

        // Assert that insights are not null
        assertNotNull(insights);
    }

    // Test case for the empty entities list
    @Test
    public void testConvertToMtnInsights_EmptyEntities() {
        List<MtnInsightsEntity> entities = new ArrayList<>();
        boolean flag = true;

        InsightsMtn insights = manager.convertToMtnInsights(entities, flag);

        // Verify no interactions with mock repositories
        verify(insightsRepository, never()).findByPkEcpdIdAndPkMtnAndPkInsightCategoryInAndPkInsightNameIn(anyString(), anyString(), anyString(), anyString());
        verify(aggrInsightRepository, never()).findByPkAggrCategoryAndPkAggrProdId(anyString(), anyString());

        // Assert that insights are empty
        assertNotNull(insights);
        assertEquals(0, insights.getCategoryInsights().length);
    }

    // Test case for JsonProcessingException in convert method
    @Test
    public void testConvertToMtnInsights_JsonProcessingException() throws SystemException {
        List<MtnInsightsEntity> entities = createNetworkOutageEntities();
        boolean flag = true;
        doThrow(new JsonProcessingException("Mock JsonProcessingException")).when(manager).convertEcpd(any());

        assertThrows(SystemException.class, () -> manager.convertToMtnInsights(entities, flag));
    }

    private List<MtnInsightsEntity> createNetworkOutageEntities() {
        List<MtnInsightsEntity> entities = new ArrayList<>();
        MtnInsightsEntity entity = new MtnInsightsEntity();

