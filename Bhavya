@Test
public void testBeforeJob_SuccessfulJobExecution() {
    JobExecution jobExecution = new JobExecution();
    jobExecution.setStartTime(new Date());
    jobExecution.setEndTime(new Date());
    jobExecution.setStatus(BatchStatus.COMPLETED);
    jobExecution.setJobInstance(new JobInstance("TestJob", "TestJobDescription"));
    
    FalloutJobExecutionListener listener = new FalloutJobExecutionListener();
    listener.beforeJob(jobExecution);
    listener.afterJob(jobExecution);
    
    // Verify log message
    verify(LOGGER).info("FalloutJobExecutionListener:: Completed TestJob-batch job in {} seconds with status: {}", 
                       anyInt(), "COMPLETED");
}