      // Check if response is successful
            if (response.getStatusCode() == HttpStatus.OK) {
                result = response.getBody();
            } else if (response.getStatusCode() == HttpStatus.NOT_FOUND) {
                // Handle 404 Not Found scenario specifically
                logger.warn("Alarm Identity not found for alarmIdentity: {}. Response: {}", alarmIdentity, response.getBody());
            } else {
                logger.error("Unexpected error occurred while calling Alarm Filtering API for alarmIdentity: {}. Response Code: {}. Response Body: {}", 
                             alarmIdentity, response.getStatusCode(), response.getBody());
            }
        } catch (RestClientException e) {
            logger.error("Exception occurred while calling Alarm Filtering API with alarmIdentity: {}. Error: {}", 
                         alarmIdentity, e.getMessage());
        } catch (Exception e) {
            logger.error("Unexpected exception for alarmIdentity: {}. Error: {}", alarmIdentity, e.getMessage());
        }
        return result;
    }