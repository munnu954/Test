import org.apache.flink.api.common.functions.MapFunction;
import org.apache.flink.api.java.DataSet;
import org.apache.flink.api.java.ExecutionEnvironment;

public class HelloWorldJob {
    public HelloWorldJob() {
    }

    public static void main(String[] args) throws Exception {
        ExecutionEnvironment env = ExecutionEnvironment.getExecutionEnvironment();
        DataSet<String> helloWorldDataSet = env.fromElements(new String[]{"Hello, World!"});
        helloWorldDataSet.map(new PrintFunction()).print();
        env.execute("HelloWorldJob");
    }

    public static class PrintFunction implements MapFunction<String, String> {
        public PrintFunction() {
        }

        public String map(String value) {
            System.out.println(value);
            return value;
        }
    }
}
