import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;
import java.util.Objects;

public class FlinkJobCompositePKTest {

    @Test
    void testEqualsAndHashCode() {
        FlinkJobCompositePK pk1 = new FlinkJobCompositePK("path1", "path2", FlinkFileTypes.TXT, true);
        FlinkJobCompositePK pk2 = new FlinkJobCompositePK("path1", "path2", FlinkFileTypes.TXT, true);

        // Test equals() method
        assertTrue(pk1.equals(pk2));
        assertTrue(pk2.equals(pk1));

        // Test hashCode() method
        assertEquals(pk1.hashCode(), pk2.hashCode());

        // Test equals() method for different object types
        assertFalse(pk1.equals("string"));

        // Test hashCode() method for null fields
        FlinkJobCompositePK pk3 = new FlinkJobCompositePK(null, "path2", FlinkFileTypes.TXT, true);
        assertNotEquals(pk1.hashCode(), pk3.hashCode());

        // Test equals() method for null fields
        assertFalse(pk1.equals(pk3));

        // Test equals() method for different field values
        FlinkJobCompositePK pk4 = new FlinkJobCompositePK("path1", "path3", FlinkFileTypes.TXT, true);
        assertFalse(pk1.equals(pk4));

        // Test equals() method for different enums
        FlinkJobCompositePK pk5 = new FlinkJobCompositePK("path1", "path2", FlinkFileTypes.CSV, true);
        assertFalse(pk1.equals(pk5));

        // Test equals() method for different enabled values
        FlinkJobCompositePK pk6 = new FlinkJobCompositePK("path1", "path2", FlinkFileTypes.TXT, false);
        assertFalse(pk1.equals(pk6));
    }
}