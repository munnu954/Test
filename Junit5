import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

class BatchLoaderUtilsTest {

    @Test
    void testIsValidData() {
        BatchLoaderUtils batchLoaderUtils = new BatchLoaderUtils();

        // Existing test cases
        assertFalse(batchLoaderUtils.isValidData(null));
        assertFalse(batchLoaderUtils.isValidData(""));
        assertFalse(batchLoaderUtils.isValidData(" "));
        assertFalse(batchLoaderUtils.isValidData("0"));
        assertTrue(batchLoaderUtils.isValidData("valid"));
        assertFalse(batchLoaderUtils.isValidData(" ")); 

        // New test case to cover the missing branch
        assertFalse(batchLoaderUtils.isValidData("   \t   ")); // String with whitespace but not empty
    }
}