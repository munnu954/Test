@Test
void testGetClrGraphicsCatchBlock() throws UnsupportedEncodingException {
    // Arrange
    String circuitId = "testCircuitId";
    String sourceSys = "testSourceSys";
    String cnd = "testCnd";

    when(clrGraphicsService.getClrGraphics(circuitId, sourceSys, cnd)).thenThrow(new UnsupportedEncodingException());

    // Act
    String response = commonWrapperController.getClrGraphics(circuitId, sourceSys, cnd);

    // Assert
    verify(clrGraphicsService).getClrGraphics(circuitId, sourceSys, cnd);
    assertEquals("", response);
    verify(logger).error("Error while parsing circuitId: {}", new UnsupportedEncodingException());
}