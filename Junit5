@Test
void testGetCircuitDetails_ResponseNotNull() {
    CircuitDetailsProvider circuitDetailsProvider = new CircuitDetailsProvider();
    RestClientUtil restClientUtil = Mockito.mock(RestClientUtil.class);
    circuitDetailsProvider.setRestClientUtil(restClientUtil);

    Mockito.when(restClientUtil.callEndpoint(Mockito.anyString(), Mockito.eq(HttpMethod.GET), Mockito.anyMap())).thenReturn("mockResponse");

    CircuitDetailResponse result = circuitDetailsProvider.getCircuitDetails("123", "sourceSys", "cnd", "phyModel");

    assertNotNull(result);
}

@Test
void testGetCircuitDetails_ResponseNull() {
    CircuitDetailsProvider circuitDetailsProvider = new CircuitDetailsProvider();
    RestClientUtil restClientUtil = Mockito.mock(RestClientUtil.class);
    circuitDetailsProvider.setRestClientUtil(restClientUtil);

    Mockito.when(restClientUtil.callEndpoint(Mockito.anyString(), Mockito.eq(HttpMethod.GET), Mockito.anyMap())).thenReturn(null);

    CircuitDetailResponse result = circuitDetailsProvider.getCircuitDetails("123", "sourceSys", "cnd", "phyModel");

    assertNull(result);
}