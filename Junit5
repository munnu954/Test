import com.verizon.uts.commonwrapper.CommonWrapperApplication;
import org.junit.jupiter.api.Test;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.cassandra.CassandraAutoConfiguration;
import org.springframework.boot.autoconfigure.cassandra.CassandraDataAutoConfiguration;
import org.springframework.boot.test.context.SpringBootTest;

import static org.mockito.Mockito.*;

class CommonWrapperApplicationTest {

    @Test
    void main() {
        // Mock SpringApplication.run
        SpringApplication springApplicationMock = mock(SpringApplication.class);
        when(springApplicationMock.run(CommonWrapperApplication.class, new String[] { "arg1", "arg2" })).thenReturn(null);

        // Call the main method and verify the mocked run method is called
        CommonWrapperApplication.main(new String[] { "arg1", "arg2" });
        verify(springApplicationMock).run(CommonWrapperApplication.class, new String[] { "arg1", "arg2" });
    }
}