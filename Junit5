@Test
void testIsDateFuture_WhenDateIsFuture_ReturnTrue() {
  // Arrange
  AlarmRepositoryImpl alarmRepository = new AlarmRepositoryImpl();
  String futureDate = "2023-01-01T12:00:00.000000Z"; // Future date
  // Act
  boolean result = alarmRepository.isDateFuture(futureDate);
  // Assert
  assertTrue(result);
}

@Test
void testIsDateFuture_WhenDateIsPast_ReturnFalse() {
  // Arrange
  AlarmRepositoryImpl alarmRepository = new AlarmRepositoryImpl();
  String pastDate = "2020-01-01T12:00:00.000000Z"; // Past date
  // Act
  boolean result = alarmRepository.isDateFuture(pastDate);
  // Assert
  assertFalse(result);
}

@Test
void testIsDateFuture_WhenDateIsCurrentDate_ReturnFalse() {
  // Arrange
  AlarmRepositoryImpl alarmRepository = new AlarmRepositoryImpl();
  Instant now = Instant.now();
  String currentDate = DateTimeFormatter.ofPattern("yyyy-MM-dd'T'HH:mm:ss.SSSSSS'Z'")
    .withZone(ZoneId.of("UTC")).format(now);
  // Act
  boolean result = alarmRepository.isDateFuture(currentDate);
  // Assert
  assertFalse(result);
}