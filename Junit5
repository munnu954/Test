@Test
void testValidateIterator_branch1() {
    MtnInsightsValidator validator = new MtnInsightsValidator();
    MtnInsightsAttributes attributes = new MtnInsightsAttributes();
    String errorMessage = null;
    String logVal = "Test";
    
    try {
        validator.validateIterator(errorMessage, attributes, logVal);
        fail("Should have thrown InvalidRequestException");
    } catch (InvalidRequestException e) {
        assertEquals("Test" + "MTN, Insight Category and Insight Name should not be null or empty.", e.getMessage());
    }
}

@Test
void testValidateIterator_branch2() {
    MtnInsightsValidator validator = new MtnInsightsValidator();
    MtnInsightsAttributes attributes = new MtnInsightsAttributes();
    attributes.setMtn("test");
    String errorMessage = null;
    String logVal = "Test";
    
    try {
        validator.validateIterator(errorMessage, attributes, logVal);
        assertNull(errorMessage);
    } catch (InvalidRequestException e) {
        fail("Unexpected exception thrown");
    }
}

@Test
void testValidateIterator_branch3() {
    MtnInsightsValidator validator = new MtnInsightsValidator();
    MtnInsightsAttributes attributes = new MtnInsightsAttributes();
    attributes.setMtn("test");
    attributes.setInsightCategory("category");
    String errorMessage = null;
    String logVal = "Test";
    
    try {
        validator.validateIterator(errorMessage, attributes, logVal);
        assertNull(errorMessage);
    } catch (InvalidRequestException e) {
        fail("Unexpected exception thrown");
    }
}
