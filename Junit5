import org.junit.jupiter.api.Test;
import org.mockito.Mockito;

import java.util.HashMap;
import java.util.Map;

import static org.junit.jupiter.api.Assertions.assertTrue;
import static org.junit.jupiter.api.Assertions.assertFalse;
import static org.mockito.Mockito.when;

public class PostingRuleTest {

    @Test
    public void testMatchFilterReturnsFalse() {
        // Create a mock PostingRule object
        PostingRule postingRule = Mockito.mock(PostingRule.class);
        
        // Create a mock alarmMap
        Map<String, String> alarmMap = new HashMap<>();
        alarmMap.put("key1", "value1");
        
        // Stub the matchFilter method to return false
        when(postingRule.matchFilter(alarmMap)).thenReturn(false);
        
        // Test the PostingRule object with the matchFilter method
        assertFalse(postingRule.matchFilter(alarmMap));
    }

    @Test
    public void testMatchFilterReturnsTrue() {
        // Create a mock PostingRule object
        PostingRule postingRule = Mockito.mock(PostingRule.class);
        
        // Create a mock alarmMap
        Map<String, String> alarmMap = new HashMap<>();
        alarmMap.put("key1", "value1");
        
        // Stub the matchFilter method to return true
        when(postingRule.matchFilter(alarmMap)).thenReturn(true);
        
        // Test the PostingRule object with the matchFilter method
        assertTrue(postingRule.matchFilter(alarmMap));
    }

}
```