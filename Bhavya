@Override
    public void flatMap(String s, Collector<String> collector) throws Exception {
        String[] arr = s.split(",");
        JSONObject obj = new JSONObject();
        int i = 1;
        String jsonkey;
        for (String input : arr) {
            jsonkey = "key" + i;
            obj.put(jsonkey, input);
            i++;
        }
        String cleanedJsonResponse = obj.toString().replaceAll("\\\\", "");
        cleanedJsonResponse = cleanedJsonResponse.replaceAll("\"", "");
        collector.collect(cleanedJsonResponse);
    }